        </sound>
        <sound name="quicksysselect">
           <path>${quicksysselectSound}</path>
        </sound>
        <sound name="select">
           <path>${selectSound}</path>
        </sound>
        <sound name="back">
           <path>${backSound}</path>
        </sound>
        <sound name="scroll">
           <path>${scrollSound}</path>
        </sound>
        <sound name="favorite">
           <path>${favSound}</path>
        </sound>
        <sound name="launch">
           <path>${launchSound}</path>
        </sound>

       
    </view>
    <view name="system,gamelist">
    <!-- Help info -->
        <helpsystem name="help">
            <pos>${helpPos}</pos>
            <origin>.5 0</origin>
            <opacity>1</opacity>
            <textColor>${baseColor}70</textColor>
            <iconColor>${highlightColor}90</iconColor>
            <fontSize>${paraFontSize}</fontSize>
            <fontPath>${helpFont}</fontPath>
            <iconTextSpacing>0.003</iconTextSpacing>
        </helpsystem>
    </view>

    <!-- Aspect Ratios -->

    

    <aspectRatio name="4:3">
        <view name="system">    
        </view> 
        <view name="gamelist">
            <video name="gamelistBgVideo">
                <cropPos>.8 0</cropPos>
            </video>
            <image name="gamelistBgImage">
                <cropPos>.8 0</cropPos>
            </image>
        </view>
    </aspectRatio>
    
</theme>